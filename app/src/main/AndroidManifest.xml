<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tugasakhir">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TugasAkhir">
        <activity android:name=".ActivityBundle"/>
        <activity android:name=".ActivityCaramel"/>
        <activity android:name=".ActivityPandan"/>
        <activity android:name=".ActivityDetailPemesanan"/>
        <activity android:name=".ActivityBlackforest"/>
        <activity android:name=".Register"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ActivityHome"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>